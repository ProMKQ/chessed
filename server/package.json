{
    "name": "server",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "description": "Chessed server",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "npx tsx --watch src/index.ts",
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix",
        "format": "prettier --write \"**/*.{js,ts,json}\"",
        "format:check": "prettier --check \"**/*.{js,ts,json}\""
    },
    "dependencies": {
        "express": "^4.21.0"
    },
    "devDependencies": {
        "@types/express": "^5.0.0",
        "@types/node": "^22.0.0",
        "tsx": "^4.0.0",
        "typescript": "^5.0.0"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}
